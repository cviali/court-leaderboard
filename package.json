{
	"name": "court-leaderboard",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"deploy:api": "cd api && npm install && wrangler deploy",
		"db:generate": "cd api && npx drizzle-kit generate",
		"db:migrate": "cd api && npx wrangler d1 migrations apply court-leaderboard --remote",
		"db:push": "npm run db:generate && npm run db:migrate",
		"db:studio": "cd api && npx drizzle-kit studio"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/modifiers": "^9.0.0",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@hookform/resolvers": "^5.2.2",
		"@opennextjs/cloudflare": "^1.14.4",
		"@radix-ui/react-avatar": "^1.1.11",
		"@radix-ui/react-checkbox": "^1.3.3",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-label": "^2.1.8",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-separator": "^1.1.8",
		"@radix-ui/react-slider": "^1.3.6",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-tabs": "^1.1.13",
		"@radix-ui/react-toggle": "^1.1.10",
		"@radix-ui/react-toggle-group": "^1.1.11",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@tabler/icons-react": "^3.36.0",
		"@tanstack/react-table": "^8.21.3",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"framer-motion": "^12.23.26",
		"lucide-react": "^0.561.0",
		"next": "15.5.9",
		"next-themes": "^0.4.6",
		"react": "19.1.4",
		"react-dom": "19.1.4",
		"react-easy-crop": "^5.5.6",
		"react-hook-form": "^7.68.0",
		"recharts": "^2.15.4",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.0",
		"tailwindcss-animate": "^1.0.7",
		"vaul": "^1.1.2",
		"zod": "^4.2.0"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251213.0",
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20.19.27",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.4.6",
		"tailwindcss": "^4",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5",
		"wrangler": "^4.54.0"
	}
}
